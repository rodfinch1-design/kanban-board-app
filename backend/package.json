{
    "name": "kanban-board-backend",
    "version": "1.0.0",
    "description": "Backend server for Kanban Board Application",
    "main": "src/server.js",
    "scripts": {
          "start": "node src/server.js",
          "dev": "nodemon src/server.js",
          "test": "jest --coverage",
          "test:watch": "jest --watch",
          "db:migrate": "node src/scripts/migrate.js",
          "db:seed": "node src/scripts/seed.js",
          "db:reset": "npm run db:migrate && npm run db:seed",
          "lint": "eslint src/",
          "lint:fix": "eslint src/ --fix"
    },
    "keywords": [
          "kanban",
          "project-management",
          "task-management",
          "express",
          "postgresql"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
          "express": "^4.18.2",
          "pg": "^8.11.3",
          "pg-hstore": "^2.3.4",
          "sequelize": "^6.35.2",
          "dotenv": "^16.3.1",
          "cors": "^2.8.5",
          "helmet": "^7.1.0",
          "bcrypt": "^5.1.1",
          "jsonwebtoken": "^9.0.2",
          "socket.io": "^4.6.1",
          "express-validator": "^7.0.1",
          "express-rate-limit": "^7.1.5",
          "multer": "^1.4.5-lts.1",
          "aws-sdk": "^2.1516.0",
          "uuid": "^9.0.1",
          "morgan": "^1.10.0",
          "compression": "^1.7.4",
          "cookie-parser": "^1.4.6",
          "express-session": "^1.17.3",
          "connect-redis": "^7.1.0",
          "redis": "^4.6.12",
          "nodemailer": "^6.9.7",
          "joi": "^17.11.0",
          "winston": "^3.11.0",
          "express-fileupload": "^1.4.3"
    },
    "devDependencies": {
          "nodemon": "^3.0.2",
          "jest": "^29.7.0",
          "supertest": "^6.3.3",
          "eslint": "^8.56.0",
          "eslint-config-airbnb-base": "^15.0.0",
          "eslint-plugin-import": "^2.29.1",
          "@types/jest": "^29.5.11"
    },
    "engines": {
          "node": ">=18.0.0",
          "npm": ">=9.0.0"
    }
}
